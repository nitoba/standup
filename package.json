{
  "name": "standup",
  "module": "src/index.tsx",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "bun run src/ai/index.ts",
    "start": "bun run --hot src/app/index.tsx",
    "build:win": "bun build --compile --target=bun-windows-x64-modern ./src/app/index.tsx --outfile ./standup-win --minify  --format=esm --external '@opentui/core'",
    "build:linux": "bun build --compile --target=bun-linux-x64-modern ./src/app/index.tsx --outfile ./standup-linux --minify --format=esm",
    "build:mac": "bun build --compile --target=bun-darwin-arm64 ./src/app/index.tsx --outfile ./standup-linux --minify --format=esm",
    "build": "bun build:win && bun build:linux",
    "check-types": "tsc -b",
    "dev:mastra": "bun run --bun mastra dev --dir ./src/ai/mastra"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@mastra/loggers": "^0.10.17",
    "mastra": "0.15.1",
    "@biomejs/biome": "^2.2.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.23",
    "@ai-sdk/groq": "^2.0.24",
    "@ai-sdk/openai-compatible": "^1.0.22",
    "@mastra/core": "0.20.2",
    "@mastra/mcp": "0.13.4",
    "@modelcontextprotocol/sdk": "^1.20.0",
    "@opentelemetry/auto-instrumentations-node": "^0.65.0",
    "@opentelemetry/core": "^2.1.0",
    "@opentelemetry/exporter-trace-otlp-grpc": "^0.206.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.206.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.206.0",
    "@opentelemetry/sdk-trace-base": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@opentui/core": "^0.1.27",
    "@opentui/react": "^0.1.27",
    "ai": "^5.0.70",
    "react": "^19.1.1",
    "zod": "^4.1.12"
  },
  "packageManager": "bun@1.3.0"
}
